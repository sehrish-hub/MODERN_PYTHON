{
    "name": "Python Dev Container",
    "dockerFile": "Dockerfile",
    "context": ".",
    "appPort": [8888],
    "postCreateCommand": "pip install -r requirements.txt",
    "settings": {
      "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
      "ms-python.python",
      "ms-toolsai.jupyter"
    ],
    "forwardPorts": [8888],
    "remoteUser": "root"
  }
  